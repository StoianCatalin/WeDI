{
  "manifest_version": 2,

  "name": "WeDI",
  "description": "This extension collects contact data from pages you visited.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "resources/icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}